{
   "$schema": "http://json-schema.org/draft-04/schema#",
   "extends": "@vue/tsconfig/tsconfig.web.json",
   "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "*.d.ts"],
   "compilerOptions": {
      "ignoreDeprecations": "5.0",
      "baseUrl": ".",
      "paths": {
         "@/*": ["./src/*"],
         "@build/*": ["build/*"]
      }
   },

   "references": [
      {
         "path": "./tsconfig.node.json"
      }
   ]
}
